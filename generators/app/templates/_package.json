{
  "name": "<%= appname %>",
  "version": "0.0.0",
  "description": "<%= appdescription %>",
  "license": "<%= applicense %>",
  "main": "",
  "repository":"<%= appname %>",
  "author": {
    "name": "<%= appauthor %>",
    "email": "<%= appemail %>",
    "url": ""
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "optimize": "gulp optimize",
    "deploy": "gulp deploy",
    "test": "mocha"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "browser-sync": "^2.16.1",
    "critical": "^0.8.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",<% if (useBabel == true) { %>
    "gulp-babel": "^6.1.1", <% } %>
    "gulp-beautify": "^2.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssimport": "^3.1.0",
    "gulp-imagemin": "^3.0.3",<% if (templateLang == 'pug') { %>
    "gulp-pug": "^3.0.4",<% } else { %>
    "gulp-htmlmin": "1.3.0", <% } %>
    "gulp-cssnano": "^2.1.2",<% if (jsLinter == 'jscs') { %>
    "gulp-jscs": "^3.0.2", <% } if (jsLinter == 'jshint') { %>
    "gulp-jshint": "^2.0.0", <% } %>
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",<% if (cssPrepro == 'less') { %>
    "gulp-less": "^3.0.5",<% } else { %>
    "gulp-sass":"^2.3.2",<% } %>
    "gulp-sourcemaps": "^2.1.1",
    "gulp-uglify": "^2.0.0",
    "gulp-uncss": "^1.0.6"<% if (useFlexboxgrid) { %> ,
    "flexboxgrid": "^6.3.1",<% } %> <% if (useBootstrap) { %>
    "bootstrap": "^3.3.6",<% } if (deployMethod == 'ftp') {%>
    "vinyl-ftp": "^0.5.0"<% } if (deployMethod == 'gh-pages') {%>
	"gulp-gh-pages": "^0.5.4"<% } if (deployMethod == 'surge') {%>
	"gulp-surge": "^0.1.0"<% } %>
  },
  "devDependencies": {
    "mocha": "*"
  }
}
